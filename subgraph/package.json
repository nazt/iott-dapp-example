{
  "name": "iott-example-subgraph",
  "version": "1.0.0",
  "repository": "git@github.com:iotexproject/pebble-subgraph.git",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types",
    "prepare:testnet": "mustache config/4690.json subgraph.template.yaml > subgraph.yaml",
    "prepare": "mustache config/4689.json subgraph.template.yaml > subgraph.yaml",
    "build": "graph build",
    "create": "graph create nazt/iott-example-subgraph --node https://rpc.graph.mainnet.iotex.io/",
    "deploy": "graph deploy nazt/iott-example-subgraph --node https://rpc.graph.mainnet.iotex.io/ --ipfs https://ipfs.mainnet.iotex.io",
    "create-local": "graph create nazt/iott-example-subgraph --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy nazt/iott-example-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.1",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "mustache": "^4.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
